{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog":
  {
    "WriteTo":
    [
      {
        "Name": "Seq",
        "Args":
        {
          "serverUrl": "{ServerURL}",
          "apiKey": "{ApiKey}"
        }
      },
      {
        "Name": "Console"
      }]
  },
  "SQL_CONNECTION_STRING": "{SQL_Connection_String}",
  "AllowedHosts": "*"
}
